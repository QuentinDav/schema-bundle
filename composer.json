{
  "name": "qd/schema-bundle",
  "description": "Interactive database schema documentation and management for Symfony. Visualize entity relationships with ER diagrams, track schema evolution across releases, collaborate with comments, and export beautiful documentation. Built with Vue 3 and Doctrine ORM.",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "symfony",
    "symfony-bundle",
    "doctrine",
    "doctrine-orm",
    "database",
    "schema",
    "documentation",
    "database-schema",
    "schema-documentation",
    "entity-relationship",
    "er-diagram",
    "erd",
    "database-visualization",
    "schema-versioning",
    "database-migration",
    "release-management",
    "changelog",
    "schema-diff",
    "collaboration",
    "team-tools",
    "developer-tools",
    "vue",
    "vue3",
    "interactive",
    "markdown-export",
    "semantic-versioning"
  ],
  "homepage": "https://github.com/QuentinDav/schema-bundle",
  "readme": "README.md",
  "authors": [
    {
      "name": "Quentin David",
      "email": "quentin.dav33@gmail.com",
      "homepage": "https://github.com/QuentinDav",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/QuentinDav/schema-bundle/issues",
    "source": "https://github.com/QuentinDav/schema-bundle",
    "docs": "https://github.com/QuentinDav/schema-bundle#readme"
  },
  "autoload": {
    "psr-4": {
      "Qd\\SchemaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qd\\SchemaBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/console": "^6.4 || ^7.0",
    "symfony/config": "^6.4 || ^7.0",
    "symfony/dependency-injection": "^6.4 || ^7.0",
    "symfony/http-foundation": "^6.4 || ^7.0",
    "symfony/security-bundle": "^6.4 || ^7.0",
    "symfony/validator": "^6.4 || ^7.0",
    "symfony/serializer-pack": "^1.3",
    "symfony/mime": "^6.4 || ^7.0",
    "symfony/http-client": "^6.4 || ^7.0",
    "doctrine/orm": "^3.5",
    "doctrine/dbal": "^3.0 || ^4.0",
    "psr/log": "^1.0 || ^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0 || ^11.0",
    "symfony/phpunit-bridge": "^6.4 || ^7.0"
  },
  "suggest": {
    "symfony/maker-bundle": "For generating custom entities",
    "symfony/security-bundle": "Required for role-based access control",
    "doctrine/doctrine-migrations-bundle": "For managing database migrations",
    "symfony/ai": "For AI-powered Natural Language to SQL generation (OpenAI, Anthropic, Azure, Gemini, VertexAI support)"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

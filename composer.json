{
  "name": "qd/schema-bundle",
  "description": "Symfony bundle for managing and serving database schemas with integrated frontend assets for team collaboration.",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["symfony-bundle", "schema"],
  "autoload": {
    "psr-4": {
      "Qd\\SchemaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qd\\SchemaBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^7.3",
    "symfony/console": "^7.3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/mime": "^7.3",
    "doctrine/orm": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  }
}
